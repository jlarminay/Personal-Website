{
  "name": "personal-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "npm run build && vite preview --port 6161",
    "sitemap": "start-server-and-test preview http://localhost:6161 sitemap:generate && npm run sitemap:replace",
    "sitemap:generate": "sitemap-generator http://localhost:6161 -v -f public/sitemap.xml",
    "sitemap:replace": "replace-in-files --string='http://localhost:6161' --replacement='https://joshlarminay.com' ./public/sitemap.xml"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vue-inline-svg": "^2.1.3",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "replace-in-files-cli": "^2.2.0",
    "sitemap-generator-cli": "^7.5.0",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  }
}
